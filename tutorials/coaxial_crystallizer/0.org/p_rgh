FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    symmetry_plane
    {
        type            symmetry;
    }
    water_inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
	value		uniform 0.0;
    }
    walls
    {
        type            zeroGradient;
    }
    solution_inlet
    {
        type            zeroGradient;
    }
}


